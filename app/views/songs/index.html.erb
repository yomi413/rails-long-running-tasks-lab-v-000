<h1>Songs</h1>

<%= form_tag songs_upload_path(@songs), multipart: true do %>
  <%= file_field_tag :songs %>
  <%= submit_tag "Upload Songs" %>
<% end %>

<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
